{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "@src/*": ["./src/*"],
      "@components": ["./src/components/*"],
      "@base-components": ["./src/base-components/*"],
      "@utils": ["./src/utils/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
