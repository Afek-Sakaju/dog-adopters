        "eslint-plugin-n": "^15.7.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.6.1",
        "mongodb-memory-server": "^8.9.3",
        "nodemon": "^2.0.19",
        "prettier": "^2.8.8",
        "supertest": "^6.2.4",
        "ts-jest": "^29.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
