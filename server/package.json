{
    "name": "dog-adopters-server",
    "description": "Server side of the Fullstack project of app for adopting lonely dogs",
    "author": "afek-sakaju",
    "homepage": "https://github.com/Afek-Sakaju/dog-adopters#readme",
    "keywords": [
        "dog-adopters",
        "adopting-dogs-site",
        "server",
        "express"
    ],
    "engines": {
        "node": ">=14.0.0"
    },
    "version": "1.0.0",
    "private": true,
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Afek-Sakaju/dog-adopters.git"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Afek-Sakaju/dog-adopters/issues"
    },
    "scripts": {
        "start": "npm run migrateDB && npm run build && node dist/app.js",
        "dev": "nodemon src/app.ts",
        "dev-with-migrate": "npm run migrateDB && nodemon src/app.ts",
        "migrateDB": "npm run migrate-mongo:up",
        "build": "tsc",
        "build:watch": "tsc -w",
        "test": "jest",
        "lint": "eslint --ext .ts .",
        "migrate-mongo:init": "migrate-mongo init",
        "migrate-mongo:up": "migrate-mongo up",
        "migrate-mongo:down": "migrate-mongo down",
        "migrate-mongo:status": "migrate-mongo status",
        "migrate-mongo:create": "migrate-mongo create init-dogs"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.0",
        "cloudinary": "^2.2.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "dotenv-expand": "^9.0.0",
        "express": "^4.18.1",
        "express-rate-limit": "^6.7.0",
        "express-session": "^1.17.3",
        "migrate-mongo": "^9.0.0",
